<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Engines.Presentation.MAUIClient.Pages.Engines.ICE.ICEEnginePage"
             xmlns:viewModels="clr-namespace:Engines.Presentation.MAUIClient.ViewModels.Engines"
             x:DataType="viewModels:ICE.ICEEngineViewModel"
             Title="ICEEnginePage">
    <VerticalStackLayout>
        <Button 
            Text="Добавить двигатель внутреннего сгорания" 
            SemanticProperties.Hint="Добавить двигатель внутреннего сгорания"
            Command="{Binding GoToAddICEEnginePageCommand}" 
            HorizontalOptions="Fill" 
            Margin="20"/>

        <Button 
            Text="Получить двигатель внутреннего сгорания" 
            SemanticProperties.Hint="Получить двигатель внутреннего сгорания"
            Command="{Binding GoToGetICEEnginePageCommand}" 
            HorizontalOptions="Fill" 
            Margin="20"/>
    </VerticalStackLayout>
</ContentPage>