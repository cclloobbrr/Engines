<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Engines.Presentation.MAUIClient.Pages.Engines.Jet.JetEnginePage"
             xmlns:viewModels="clr-namespace:Engines.Presentation.MAUIClient.ViewModels.Engines"
             x:DataType="viewModels:Jet.JetEngineViewModel"
             Title="JetEnginePage">
    <VerticalStackLayout>
        <Button 
            Text="Добавить реактивный двигатель" 
            SemanticProperties.Hint="Добавить реактивный двигатель"
            Command="{Binding GoToAddJetEnginePageCommand}" 
            HorizontalOptions="Fill" 
            Margin="20"/>

        <Button 
            Text="Получить реактивный двигатель" 
            SemanticProperties.Hint="Получить реактивный двигатель"
            Command="{Binding GoToGetJetEnginePageCommand}" 
            HorizontalOptions="Fill" 
            Margin="20"/>
    </VerticalStackLayout>
</ContentPage>